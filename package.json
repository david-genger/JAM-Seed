    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:webpack && npm run build:metalsmith",
    "build:prod": "cross-env NODE_ENV=production && npm run clean && npm run build",
    "build:metalsmith": "babel-node scripts/build-metalsmith.js",
    "build:webpack": "webpack --config webpack.prod.js",
    "dev": "nodemon --watch scripts --exec babel-node scripts/run-dev-server.js",
    "debug": "babel-node --inspect-brk scripts/run-dev-server.js",
    "server": "npm run build && http-server dist/site",
    "timer-dev": "cross-env DEBUG=metalsmith-timer nodemon --watch scripts --exec babel-node scripts/run-dev-server.js",
    "server:prod": "npm run build:prod && http-server dist",
    "clean": "npm run clean:metalsmith && npm run clean:webpack",
    "clean:metalsmith": "rimraf dist/site/*",
    "clean:webpack": "rimraf dist/assets/*"
  },
